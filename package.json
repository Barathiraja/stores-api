{
    "name": "stores-api",
    "version": "1.0.0",
    "description": "Stores API",
    "author": "Barathirajan M",
    "main": "build/store-api.js",
    "scripts": {
        "build:dev": "npm run clean:dist && webpack --mode=development",
        "build:prod": "npm run clean:dist && webpack --mode=production",
        "clean:build": "rimraf build",
        "clean:dist": "rimraf dist",
        "lint": "npm run tslint \"src/**/*.ts\"",
        "pree2e": "webdriver-manager update",
        "start": "nodemon -V --exec ts-node src/main.ts",
        "tslint": "tslint",
        "test": "mocha -r ts-node/register test/**/*.ts --reporter mochawesome --reporter-options reportDir=reports,reportFilename=test",
        "cover": "nyc npm run test"
    },
    "keywords": [
        "typescript",
        "mocha",
        "sourcemaps",
        "node",
        "npm scripts",
        "tsc"
    ],
    "dependencies": {
        "@okta/jwt-verifier": "0.0.14",
        "@types/body-parser": "1.17.0",
        "@types/compression": "0.0.36",
        "@types/cors": "2.8.4",
        "@types/express": "4.11.1",
        "@types/node": "10.3.0",
        "body-parser": "1.18.3",
        "chai-http": "^4.3.0",
        "compression": "1.7.2",
        "copy-webpack-plugin": "4.5.1",
        "cors": "^2.8.4",
        "express": "4.16.3",
        "express-winston": "2.5.1",
        "fs": "0.0.1-security",
        "joi": "^14.3.1",
        "jsonwebtoken": "^8.3.0",
        "mongoose": "^5.7.1",
        "nconf": "0.10.0",
        "path": "0.12.7",
        "rimraf": "^2.6.2",
        "swagger-ui-express": "3.0.10",
        "ts-loader": "4.4.1",
        "ts-node": "6.1.0",
        "tslint": "5.10.0",
        "typescript": "2.9.1",
        "webpack": "^4.12.0",
        "webpack-cli": "^3.0.3",
        "webpack-node-externals": "^1.7.2",
        "winston": "2.4.2",
        "winston-daily-rotate-file": "3.2.1"
    },
    "devDependencies": {
        "@types/chai": "^4.2.3",
        "@types/mocha": "^5.2.7",
        "@webpack-cli/migrate": "^0.0.8",
        "chai": "^4.2.0",
        "mocha": "^6.2.0",
        "mochawesome": "^4.1.0",
        "nodemon": "^1.18.9",
        "nyc": "^14.1.1",
        "webdriver-manager": "12.0.6"
    }
}
